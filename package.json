{
  "name": "angular",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4000",
    "start:shell": "ng serve shell",
    "start:auth": "ng serve auth",
    "start:home": "ng serve home",
    "start:user-portal": "ng serve user-portal",
    "build": "ng build",
    "build:dev": "ng build shell --configuration development && ng build auth --configuration development && ng build home --configuration development",
    "build:stage": "ng build shell --configuration stage && ng build auth --configuration stage && ng build home --configuration stage",
    "build:prod": "ng build shell && ng build auth && ng build home",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:shell": "jest --coverage shell",
    "test:auth": "jest --coverage auth",
    "test:home": "jest --coverage home",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:all": "npm-run-all --parallel start:auth start:home start:shell start:user-portal"
  },
  "dependencies": {
    "@angular-architects/module-federation": "^14.2.3",
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@ngrx/effects": "^14.3.3",
    "@ngrx/store": "^14.3.3",
    "primeicons": "6.0.1",
    "primeng": "14.2.3",
    "rxjs": "^7.5.5",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.0",
    "@angular/cli": "~14.2.12",
    "@angular/compiler-cli": "^14.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "16.11.7",
    "dotenv": "^16.4.5",
    "jest": "^29.5.0",
    "jest-preset-angular": "^13.0.0",
    "ngx-build-plus": "^14.0.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.4.13",
    "ts-jest": "^29.0.5",
    "typescript": "~4.7.2"
  }
}